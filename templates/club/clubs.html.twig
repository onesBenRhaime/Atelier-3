{% extends 'base.html.twig' %}

{% block title %}Clubs{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">


<table border="1">
  <tr>
    <th> Id</th>
    <th>Name</th>
    <th>Creation Date</th>
    <th>Actions</th>
  
  </tr>
  {% for ele in clubs %}
    <tr>
      <td> {{ele.id}}</td>
      <td> {{ele.name}}</td>
      <td> {{ele.creationDate|date}}</td>
     <td> 
        <a  href=" {{ path('app_club_get',{'id':ele.id}) }}"> details</a>  |  <a  href=" {{ path('app_club_remove',{'id':ele.id}) }}"> remove</a>
     </td>
    </tr>
 {% endfor %}
</table>


</div>
{% endblock %}