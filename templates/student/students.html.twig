{% extends 'base.html.twig' %}

{% block title %}Hello StudentController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
<h3>Liste des Etudents</h3>
<table border="1">
  <tr>
    <th> NSC</th>
    <th>Email</th>
    <th> Actions</th>
  </tr>
  {% for ele in Students %}
    <tr>
      <td> {{ele.NSC}}</td>
      <td> {{ele.email}}</td>
      <td> 
        <a  href=" {{ path('student_get',{'id':ele.NSC}) }}"> info</a>  |  
        <a  href=" {{ path('student_remove',{'id':ele.NSC}) }}"> supp</a>
     </td>
      
    </tr>
 {% endfor %}
</table>


</div>
{% endblock %}